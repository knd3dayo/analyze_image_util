{
  "mcpServers": {
    "analyze_image_mcp": {
      "disabled": false,
      "timeout": 60,
      "type": "stdio",
      "command": "uv",
      "args": [
        "--directory",
        "<PATH_TO_VENV>",
        "run",
        "-m",
        "analyze_image_mcp.mcp_modules.mcp_app_server"
      ],
      "env": {
        "LLM_PROVIDER": "openai",
        "OPENAI_API_KEY": "sk-****************",
        "OPENAI_COMPLETION_MODEL": "gpt-4o-mini",
        "OPENAI_EMBEDDING_MODEL": "text-embedding-3-small"
      }
    }
  }
}